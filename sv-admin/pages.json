  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "sv-admin",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "h5": {
      "titleNView": false
    }
  },
  "topWindow": {
    "path": "layout/navbar",
    "style": {
      "height": "50px"
    },
    "matchMedia": {
      "minWidth": 0
    }
  },
  "leftWindow": {
    "path": "layout/sidebar",
    "style": {
      "width": "200px"
    }
  },
  "uniIdRouter": {
    "loginPage": "uni_modules/sv-id-pages/pages/login/login-admin",
    "needLogin": [
      "^(?!.*(sv-id-pages\/pages\/login|sv-id-pages\/pages\/agreements|pages\/error|pages\/web)).*$"
    ],
    "resToLogin": true
  }
}